@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="container">
    <form method="post" class="w-50">
        <label asp-for="Title"></label>
        <input
            asp-for="Title"
            class="form-control mb-3 me-4"
            aria-required="true"/>
        <label asp-for="Type"></label>
        <input
            asp-for="Type"
            class="form-control mb-3 me-4"
            aria-required="true"/>
        
        <button type="submit" class="btn btn-primary">Submit</button>

    </form>

</div>